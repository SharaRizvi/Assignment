<script>

    $(document).ready(function () {

        $("#upload_doc_form").on("submit", function (e) {
            e.preventDefault();
            var formData = new FormData($('#upload_doc_form')[0]);

            formData.append('file', $('input[type=file]')[0].files[0]);

            $.ajax({
                type: "POST",
                url: "/file/upload",
                data: formData,
                //use contentType, processData for sure.
                contentType: false,
                processData: false,
                success: function (msg) {
                    window.location.hash = "tab-4";
                }
            });
        });
    });
</script>

