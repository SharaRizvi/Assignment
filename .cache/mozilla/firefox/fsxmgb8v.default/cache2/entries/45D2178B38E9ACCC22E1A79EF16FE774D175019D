<script type="text/javascript">

    $('#top-search').typeahead({
        ajax: '/employee/searchEmployee',
        display: 'name',
        onSelect: function (item) {
            var id = item.value;
            var url = "/employee/show/";
            window.location.href = url + id;
        },
        val: 'id',
        grepper: function (data) {
            var that = this;
            if (data && data.length) {
                items = $.grep(data, function (item) {
                    return that.matcher(item['username']) || that.matcher(item['name']) || that.matcher(item['employeeId']);
                });
                return items;
            }
            return null;

        }

    });
    $('#top-search').on('keyup keypress', function (e) {
        var keyCode = e.keyCode || e.which;
        if (keyCode === 13) {
            e.preventDefault();
            return false;
        }
    });


</script>
