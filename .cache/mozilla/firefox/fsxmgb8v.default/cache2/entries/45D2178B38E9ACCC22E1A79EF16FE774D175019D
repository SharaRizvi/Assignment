    <script>
        function showError(message) {
            $('#toast-type').text("error");
            $('#toast-message').text(message);
            $("#showtoast").click();
        }

        function showInfo(message) {
            $('#toast-type').text("info");
            $('#toast-message').text(message);
            $("#showtoast").click();
        }

        function showSuccess(message) {
            $('#toast-type').text("success");
            $('#toast-message').text(message);
            $("#showtoast").click();
        }

        function showWarning(message) {
            $('#toast-type').text("warning");
            $('#toast-message').text(message);
            $("#showtoast").click();
        }

        function getResponseMap(data) {
            var dat = JSON.stringify(data);
            return JSON.parse(dat);
        }

        // Global Ajax events fire in
        $(document).ajaxError(function (e, xhr, opts) {
            if (xhr.status == 401) { // UnAuthorised Access to ajax resource
                window.location.reload();
                return;
            }
        });

    </script>
    
    
