<div class="modal inmodal" id="workflowDocumentCommentModal" tabindex="-1" role="dialog" aria-hidden="true">
</div>
<script type="text/javascript">

    $(".document-preview").on('click', function () {
        var docUrl = $(this).attr("docUrl");
        var docContentType = $(this).attr("docContentType")
        if(docContentType=='IMAGE')
        {
             $('#previewUrl').html('<img alt="image" class="img-responsive" src='+docUrl+'>')
        }
        else
        {
            $('#previewUrl').html('<iframe src="http://docs.google.com/viewer?url='+docUrl+'&embedded=true"   class="embedFileStyle"></iframe>')
        }
        $('#documentPreviewModal').modal("show");

    });
    $(document).on('click', '.cancelButton', function () {
        var taskId = $(this).parent().closest('tr').find("input").val();
        $('#documentPreviewModal').modal("hide");

    });
    $(document).on('click', '.fileUploadSubmitButton', function () {
        var extStatus = false
        var ext = $('.changeFile').val().match(/\.([^\.]+)$/)[1].toLowerCase();
        var fileSize = $(".changeFile")[0].files[0].size
        switch (ext) {
            case 'jpg':
            case 'jpeg':
            case 'png':
            case 'doc':
            case 'docx':
            case 'pdf':
                extStatus = true;
                break;
            default:
                extStatus = false;
        }
        if (extStatus == false) {
            showError('The selected file type is not supported. Please select another file');
            removeSelection();
            return false
        }
        if (fileSize == 0) {
            showError('Please select a file in the given size range (1 Kb to 2 Mb)');
            removeSelection();
            return false
        }
        if (fileSize > 2000000) {
            showError('Please select a file in the given size range (1 Kb to 2 Mb)');
            removeSelection();
            return false
        }
        return true
    })
    function removeSelection() {
        $('.changeFile').val('');
        $('.fileinput-filename').text('');
        $('.fileUploadSubmitButton').attr('disabled', false);
    }
    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip({placement: 'left'});
    })
</script>
