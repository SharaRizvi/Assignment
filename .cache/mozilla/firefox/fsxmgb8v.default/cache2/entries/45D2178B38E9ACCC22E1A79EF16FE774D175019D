<script>
    var dataTable
    function formatDate(date) {
        var monthNames = [
            "Jan", "Feb", "March",
            "Apr", "May", "Jun", "Jul",
            "Aug", "Sep", "Oct",
            "Nov", "Dec"
        ];

        var day = date.getDate();
        var monthIndex = date.getMonth();
        var year = date.getFullYear();

        return day + '-' + monthNames[monthIndex] + '-' + year;
    }
    $(document).ready(function () {
        $("input[name=startDateIO]").datepicker({
            autoclose: true
        })
        $("input[name=endDateIO]").datepicker({
            autoclose: true
        })
        var startDate = new Date();
        var endDate = new Date();
        startDate.setDate(endDate.getDate()-29);
        $("input[name=startDateIO]").datepicker("setDate", formatDate(startDate))
        $("input[name=endDateIO]").datepicker("setDate", formatDate(endDate))

        $("input[name=startDateIO]").val(formatDate(startDate))
        $("input[name=endDateIO]").val(formatDate(endDate))

        inOutDetails()

    });


   function inOutDetails() {
//       console.log($('input[name=startDateIO]').val());
        var st =  new Date($('input[name=startDateIO]').val())
        var et = new Date($('input[name=endDateIO]').val())
       var startDateTime = Math.abs(st.getTime());
       var endDateTime = Math.abs(et.getTime());
       if (startDateTime > endDateTime) {
            showError("The end date cannot be smaller than the start date.")
        }else {
       dataTable = $('#inOutTabTable').dataTable({
           'bSort': false,
           'ajax': {
               url: '/dashboard/getInOutDetails',
               data: {
                   empId:$("#empId").val(),
                   startDate: $('input[name=startDateIO]').val(),
                   endDate: $('input[name=endDateIO]').val()
               },
               dataSrc:function (response) {
                   $("#avgWorkingHours").text(response.avgWorkingHours)
                   $("#totalWorkingDays").text(response.totalWorkingDays)
                   return response.data;
               }

           },
//           "pagingType": "full_numbers",
//           "serverSide": true,
           "processing": true,
           destroy: true,
           responsive: true,
           dom: "ltpr"
       });
       var buttons = new $.fn.dataTable.Buttons(dataTable, {
           buttons: [
               {extend: 'csv', title: 'In-Out Details'},
               {extend: 'excel', title: 'In-Out Details'}
           ]
       }).container().appendTo($('#buttons'));
   }
   }

</script>


